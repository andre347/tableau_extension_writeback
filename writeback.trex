<!--?xml version="1.0" encoding="utf-8"?-->
<manifest manifest-version="0.1" 
    xmlns="http://wwww.tableau.com/xml/extension_manifest">
    <dashboard-extension id="com.theinformationlab.extensions.writeback" extension-version="1.0.0">
        <default-locale>en_US</default-locale>
        <name resource-id="name"></name>
        <description>Write Back to Local-File</description>
        <author name="Andre de Vries" email="andre.devries@theinformationlab.co.uk" organization="The Information Lab" website="https://www.theinformationlab.co.uk"></author>
        <min-api-version>1.1</min-api-version>
        <source-location>
            <url>http://localhost:3000/addData</url>
        </source-location>
        <icon></icon>
    <permissions>
      <permission>full data</permission>
    </permissions>
    </dashboard-extension>
    <resources>
        <resource id="name">
            <text locale="en_US">Write_Back</text>
            <text locale="fr_BE">Write_Back</text>
            <text locale="de_DE">Write_Back</text>
        </resource>
     </resources>   
</manifest>